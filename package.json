    "vue": "^3.2.25",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "cross-env": "^7.0.3",
    "husky": "^8.0.1",
    "less": "^4.1.2",
    "lint-staged": "^12.4.1",
    "naive-ui": "^2.28.2",
    "prettier": "^2.6.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.7",
    "vite-plugin-mock": "^2.9.6",
    "vue-tsc": "^0.34.7"
