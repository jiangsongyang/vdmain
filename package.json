{
  "name": "v-dmain",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "npx eslint .",
    "fix": "npx prettier . --write",
